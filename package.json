{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-cbord-get",
  "title": "CBORD Get",
  "description": "Check your CBORD GET account for available meal swipes and remaining balance.",
  "icon": "command-icon.png",
  "author": "jasonaa",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Account Details",
      "description": "View your remaining balances and transactions",
      "mode": "view"
    },
    {
      "mode": "no-view",
      "title": "Accounts",
      "name": "overview",
      "description": "View your remaining balances",
      "interval": "2h"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "type-fest": "^4.9.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
